<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="blazor-nuget" value="https://pkgs.dev.azure.com/msdnprof1492license/ej2-blazor-source/_packaging/blazor-production/nuget/v3/index.json" />
    <add key="NuGet.org" value="https://api.nuget.org/v3/index.json" />
    <!--<add key="NexusServer" value="http://nexus.syncfusion.com/repository/nuget-hosted/" />-->
  </packageSources>
  <config>
    <add key="dependencyVersion" value="Highest" />
  </config>
  <packageRestore>
      <add key="enabled" value="True" />
      <add key="automatic" value="True" />
    </packageRestore>
    <disabledPackageSources />
    <activePackageSource>
      <add key="All" value="(Aggregate source)" />
    </activePackageSource>
</configuration>
