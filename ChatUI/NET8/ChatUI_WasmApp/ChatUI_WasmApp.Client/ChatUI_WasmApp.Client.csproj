<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <NoDefaultLaunchSettingsFile>true</NoDefaultLaunchSettingsFile>
    <StaticWebAssetProjectMode>Default</StaticWebAssetProjectMode>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="8.0.22" />
	<PackageReference Include="Markdig" Version="0.42.0" />
	<PackageReference Include="Syncfusion.Blazor.InteractiveChat" Version="31.2.12" />
	<PackageReference Include="Syncfusion.Blazor.Navigations" Version="31.2.12" />
	<PackageReference Include="Syncfusion.Blazor.Themes" Version="31.2.12" />
  </ItemGroup>

</Project>
